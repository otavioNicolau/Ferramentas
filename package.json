{
  "name": "muiltools",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "npm run robots:generate && next build",
    "build:netlify": "set CI=false && npm run robots:generate && next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next export",
    "netlify-build": "npm run build",
    "sitemap:generate": "node scripts/generate-sitemap.js",
    "sitemap:stats": "node scripts/generate-sitemap.js && cat public/sitemap-stats.json",
    "robots:generate": "node scripts/generate-robots.js",
    "seo:generate": "npm run robots:generate",
    "seo:validate": "npm run seo:generate && echo 'SEO files generated successfully!'",
    "vps:diagnose": "node diagnose-vps.js",
    "vps:install": "npm cache clean --force && rm -rf node_modules package-lock.json && npm install --legacy-peer-deps",
    "vps:install-prod": "npm install --only=production --no-audit --no-fund --legacy-peer-deps",
    "vps:install-dev": "npm install --only=development --no-audit --no-fund --legacy-peer-deps",
    "vps:setup": "cp .npmrc-vps .npmrc && npm run vps:install",
    "vps:build": "npm run vps:install-prod && npm run build"
  },
  "dependencies": {
    "@distube/ytdl-core": "^4.16.12",
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@ffmpeg/util": "^0.12.2",
    "@heroicons/react": "^2.2.0",
    "@tobyg74/tiktok-api-dl": "^1.3.3",
    "@types/file-saver": "^2.0.7",
    "canvas": "^3.1.2",
    "cors": "^2.8.5",
    "docx": "^9.5.1",
    "epubjs": "^0.3.93",
    "express": "^5.1.0",
    "ffmpeg-static": "^5.2.0",
    "file-saver": "^2.0.5",
    "fluent-ffmpeg": "^2.1.3",
    "fs-extra": "^11.3.1",
    "html2canvas": "^1.4.1",
    "html5-qrcode": "^2.3.8",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "jszip": "^3.10.1",
    "lucide-react": "^0.536.0",
    "mammoth": "^1.10.0",
    "next": "15.4.5",
    "node-fetch": "^3.3.2",
    "pdf-lib": "^1.17.1",
    "pdf2pic": "^3.2.0",
    "pdfjs-dist": "^5.4.54",
    "qr-scanner": "^1.4.2",
    "qrcode": "^1.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-webcam": "^7.2.0",
    "tesseract.js": "^6.0.1",
    "tiktok-downloader": "^0.0.4",
    "xlsx": "^0.18.5",
    "xml2js": "^0.6.2",
    "@eslint/eslintrc": "^3",
    "@netlify/plugin-nextjs": "^5.7.2",
    "@tailwindcss/postcss": "^4",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/fs-extra": "^11.0.4",
    "@types/jszip": "^3.4.0",
    "@types/node": "^20",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "devDependencies": {}
}